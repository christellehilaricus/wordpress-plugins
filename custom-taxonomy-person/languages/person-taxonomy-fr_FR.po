#: custom-taxonomy-person.php:106 custom-taxonomy-person.php:220
msgid "Real name"
msgstr "Vrai nom"

#: custom-taxonomy-person.php:108 custom-taxonomy-person.php:223
msgid "Enter the real name of the person."
msgstr "Entrer le vrai nom de la personne."

#: custom-taxonomy-person.php:112 custom-taxonomy-person.php:228
msgid "Given name"
msgstr "Prénom"

#: custom-taxonomy-person.php:114 custom-taxonomy-person.php:231
msgid "Enter the given name of the person."
msgstr "Entrer le prénom de la personne."

#: custom-taxonomy-person.php:118 custom-taxonomy-person.php:236
msgid "Family name"
msgstr "Nom de famille"

#: custom-taxonomy-person.php:120 custom-taxonomy-person.php:239
msgid "Enter the family name of the person."
msgstr "Entrer le nom de famille de la personne."

#: custom-taxonomy-person.php:124 custom-taxonomy-person.php:244
msgid "Name order"
msgstr "Ordre du nom"

#: custom-taxonomy-person.php:126 custom-taxonomy-person.php:247
msgid ""
"Enter name order: 0 for \"Given name; Last name\", 1 for \"Last name; Given "
"name\"."
msgstr ""
"Entrer l'ordre du nom complet : 0 pour \"Prénom ; Nom de famille\", 1 pour "
"\"Nom de famille ; Prénom\"."

#: custom-taxonomy-person.php:130 custom-taxonomy-person.php:252
msgid "Gender"
msgstr "Genre"

#: custom-taxonomy-person.php:132
msgid "Enter the gender of the person, 0 for male and 1 for female."
msgstr "Entrer le genre de la personne, 0 pour masculin et 1 pour féminin."

#: custom-taxonomy-person.php:136 custom-taxonomy-person.php:260
msgid "Birthdate"
msgstr "Date de naissance"

#: custom-taxonomy-person.php:138 custom-taxonomy-person.php:144
#: custom-taxonomy-person.php:263 custom-taxonomy-person.php:271
msgid "Enter a date with the format YYYY-mm-dd."
